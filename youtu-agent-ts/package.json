{
  "name": "youtu-agent-ts",
  "version": "1.0.0",
  "description": "Node.js TypeScript版本的youtu-agent智能体框架",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "cli": "tsx src/cli.ts",
    "serve": "npm run cli serve",
    "example": "npm run cli example",
    "example:orchestra": "tsx examples/orchestra_agent_example.ts",
    "example:database": "tsx examples/database_example.ts",
    "example:database-no-sqlite": "tsx examples/database_example_no_sqlite.ts",
    "example:simple-no-database": "tsx examples/simple_example_no_database.ts",
    "example:simple-search": "tsx examples/simple_search_example.ts",
    "example:mock-search": "tsx examples/simple_mock_search.ts",
    "example:string-search": "tsx examples/simple_search_string.ts",
    "example:workforce-search": "tsx examples/simple_workforce_search.ts",
    "example:direct-search": "tsx examples/simple_direct_search.ts",
    "example:inline-search": "tsx examples/simple_inline_search.ts",
    "example:mock-complete": "tsx examples/simple_mock_complete.ts",
    "example:basic": "tsx examples/simple_basic_example.ts",
    "example:direct": "tsx examples/simple_direct_example.ts",
    "example:glob": "tsx examples/glob_example.ts",
    "example:workforce": "tsx examples/workforce_simple_example.ts",
    "example:glob-no-database": "tsx examples/glob_example_no_database.ts",
    "install:sqlite3": "bash scripts/install-sqlite3.sh"
  },
  "keywords": [
    "ai",
    "agent",
    "typescript",
    "nodejs",
    "llm",
    "langchain"
  ],
  "author": "youtu-agent-ts",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^11.0.3",
    "helmet": "^7.1.0",
    "langchain": "^0.3.0",
    "multer": "^1.4.5-lts.1",
    "puppeteer": "^24.10.2",
    "serve": "^14.2.5",
    "sharp": "^0.33.0",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
